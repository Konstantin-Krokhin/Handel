<Window x:Class="HandelTSE.ViewModels.ProgressBarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HandelTSE.ViewModels"
        mc:Ignorable="d"
        Title="ProgressBarWindow" Height="20" Width="120" WindowStyle="None" ResizeMode="NoResize">
    <Grid>
        <ProgressBar Minimum="0" Maximum="100" Name="pbStatus" Visibility="Visible" Height="20" Width="120" IsIndeterminate="True" />
    </Grid>
</Window>
